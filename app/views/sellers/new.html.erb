<h1>New Product</h1>

<%=form_with model:@product, url: sellers_path ,method: :post do |form|%>
  <div>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :price %><br>
    <%= form.text_field :price %><br>
  </div>
   <div>
    <%= form.label :stok %><br>
    <%= form.number_field :stok %><br>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
